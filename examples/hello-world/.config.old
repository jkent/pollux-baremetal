CONFIG_BAREMETAL_USE_LINKER_SCRIPT=y
CONFIG_BAREMETAL_BOOT_SOURCE_RAM=y
# CONFIG_BAREMETAL_BOOT_SOURCE_UART is not set
CONFIG_BAREMETAL_SHADOW=y
